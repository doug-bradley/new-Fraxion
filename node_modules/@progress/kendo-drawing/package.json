{
  "name": "@progress/kendo-drawing",
  "description": "Kendo UI Drawing",
  "homepage": "https://www.telerik.com/kendo-ui",
  "version": "1.23.0",
  "main": "dist/npm/main.js",
  "module": "dist/es/main.js",
  "jsnext:main": "dist/es/main.js",
  "es2015": "dist/es2015/main.js",
  "typings": "dist/npm/main.d.ts",
  "jsdelivr": "dist/cdn/main.js",
  "unpkg": "dist/cdn/main.js",
  "sideEffects": false,
  "scripts": {
    "start": "gulp start",
    "api-check": "gulp api-check",
    "api": "gulp api",
    "build": "gulp build-module && gulp build-cdn",
    "test": "gulp test && gulp test-typings",
    "test:ci": "npm run test",
    "e2e": "gulp watch-e2e",
    "e2e:ci": "cross-env CHROME_BIN=`which chrome || which chromium-browser` ENV_BROWSER=Chrome_headless gulp e2e",
    "lint": "eslint . --quiet",
    "verify-npm": "gulp verify-npm",
    "prepare": "husky"
  },
  "keywords": [
    "Kendo UI",
    "Drawing",
    "Vector Graphics",
    "SVG",
    "Canvas"
  ],
  "dependencies": {
    "@progress/kendo-common": "^1.0.1",
    "@progress/pako-esm": "^1.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@progress/kendo-file-saver": "^1.0.0",
    "@progress/kendo-package-tasks": "^5.0.0",
    "@progress/kendo-theme-default": "^2.0.0",
    "@progress/kendo-typescript-tasks": "^12.0.0",
    "@types/node": "8.10.11",
    "cross-env": "^7.0.3",
    "eslint": "^8.17.0",
    "eslint-plugin-import": "^2.26.0",
    "gulp": "^5.0.0",
    "gulp-typescript": "^5.0.1",
    "husky": "^7.0.1",
    "semantic-release": "^24.2.3",
    "typescript": "~4.6.0"
  },
  "author": "Progress",
  "license": "See license in LICENSE.md",
  "@progress": {
    "friendlyName": "Drawing"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-drawing.git"
  },
  "bugs": {
    "url": "https://github.com/telerik/kendo-angular"
  },
  "files": [
    "dist",
    "*.d.ts",
    "geometry.js",
    "main.js",
    "pdf.js"
  ]
}
